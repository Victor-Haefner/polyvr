      <core>
	import VR
	
	r = VR.getRoot().find('Headlight')
	if hasattr(VR, 'test'):
		print 'destroy'
		VR.test.destroy()
	VR.test = VR.Object('test')
	r.addChild(VR.test)
	
	def doParticles():
		p = VR.Particles()	
		VR.test.addChild(p)
	
	def doGround():
		ground = VR.Geometry('ground')
		ground.setPrimitive('Box 1 1 1 1 1 1')
		VR.test.addChild(ground)
	
		ground.translate(0,-1,0)
		ground.setCollisionMargin(0.01)
		ground.physicalize(1,0,0)
		
	doGround()
	doParticles()
	
