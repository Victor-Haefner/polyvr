    <Script base_name="initScanner" name_space="__script__" name_suffix="0" persistency="666" unique="1" type="Python" server="server1">
      <core>
	import VR
	
	green = VR.Material('gray')
	green.setDiffuse(0.8,1.0,0.8)
	gray = VR.Material('gray')
